%ul.sub_nav#questions_list
  - if @questions.size > 0
    - @questions.each do |question|
      %li.hoverable.q_selectable{:data => {:question_id => question.id}}
        %a.selectable{:href => "#"}
          = question.description
  - else
    = "No Questions in question bank"       
#question_show
= link_to "#", "Add to Quiz", :id => 'add_to_quiz', :'data-qid' => ''